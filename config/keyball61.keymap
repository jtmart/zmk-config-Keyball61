    combos {
        compatible = "zmk,combos";

        Lang_1_EN {
            bindings = <&kp LANGUAGE_1>;
            key-positions = <30 29>;
        };

        Lang_2_KR {
            bindings = <&kp LANGUAGE_2>;
            key-positions = <17 18>;
        };

        Lang_3_HI {
            bindings = <&kp LANGUAGE_3>;
            key-positions = <5 6>;
        };

        Minus {
            bindings = <&kp MINUS>;
            key-positions = <1 2>;
        };

        Plus {
            bindings = <&kp PLUS>;
            key-positions = <3 4>;
        };

        Equal {
            bindings = <&kp EQUAL>;
            key-positions = <7 8>;
        };

        System_reset {
            bindings = <&sys_reset>;
            key-positions = <11 10 9>;
        };

        Bootloader_mode {
            bindings = <&bootloader>;
            key-positions = <0 1 2>;
        };

        Power_on {
            bindings = <&ext_power EP_ON>;
            key-positions = <23 22 21>;
        };

        Power_off {
            bindings = <&ext_power EP_OFF>;
            key-positions = <12 13 14>;
        };
    };

    macros {
        Open_terminal: Open_terminal {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LCTRL>,
                <&macro_press>,
                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp NUMBER_4>;

            label = "OPEN_TERMINAL";
        };

        Screenshot: Screenshot {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LEFT_WIN>,
                <&macro_press>,
                <&kp LEFT_SHIFT>,
                <&macro_press>,
                <&kp S>;

            label = "SCREENSHOT";
        };

        em_dash: em_dash {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&macro_press>,
                <&kp LC(LSHFT)>,
                <&macro_tap>,
                <&kp U>,
                <&macro_release>,
                <&kp LC(LSHFT)>,
                <&macro_tap>,
                <&kp N2 &kp N0 &kp N1 &kp N4 &kp RET>;

            label = "EM_DASH";
        };

        Open_calibre: Open_calibre {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LCTRL>,
                <&macro_press>,
                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp C>;

            label = "OPEN_CALIBRE";
        };

        Open_browser: Open_browser {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LCTRL>,
                <&macro_press>,
                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp B>;

            label = "OPEN_BROWSER";
        };

        Pipe_operator: Pipe_operator {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp PIPE>, <&macro_tap>, <&kp GREATER_THAN>;

            label = "PIPE_OPERATOR";
        };

        Open_something: Open_something {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LCTRL>,
                <&macro_press>,
                <&kp LEFT_ALT>,
                <&macro_press>,
                <&kp R>;

            label = "OPEN_SOMETHING";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
            label = "QWRT not Colemak";
            bindings = <
&HML ESCAPE GRAVE    &CLBR 0 N1  &HML C_BRIGHTNESS_DEC N2  &HML C_BRIGHTNESS_INC N3  &BLTH BT_PRV N4  &BLTH BT_NXT N5                                         &BCKL BL_DEC N6      &BCKL BL_INC N7       &HML K_MUTE N8  &HML N9 C_VOLUME_DOWN  &HML C_VOLUME_UP N0  &HML K_POWER K_SLEEP
&kp TAB              &HML F2 Q   &HML F11 W                &BRWSR 0 E                &SMTHNG 0 R      &TRMNL 0 T                                              &HML MINUS Y         &HML PLUS U           &kp I           &kp O                  &PIPE_OPERATOR 0 P   &kp BACKSLASH
&kp CAPSLOCK         &kp A       &HML PRINTSCREEN S        &kp D                     &kp F            &kp G                                                   &HML LEFT_BRACKET H  &HML RIGHT_BRACKET J  &kp K           &kp L                  &kp SEMI             &kp SQT
&kp LEFT_SHIFT       &kp Z       &kp X                     &kp C                     &kp V            &kp B            &HML PAGE_DOWN LS(LG(S))  &kp PAGE_UP  &HML LEFT_BRACE N    &HML RIGHT_BRACE M    &kp COMMA       &kp DOT                &kp FSLH             &kp RIGHT_SHIFT
&SH_EM_DASH TILDE 0  &kp LCTRL   &kp LEFT_ALT              &kp LEFT_WIN              &mo 6            &lt 4 SPACE      &mo 5                     &kp ENTER    &lt 2 BACKSPACE                                                                   &kp RIGHT_ALT        &kp DELETE
            >;

            sensor-bindings =
                <&SCROLL_UP_DOWN>,
                <&inc_dec_kp C_VOLUME_UP C_VOLUME_DOWN>;
